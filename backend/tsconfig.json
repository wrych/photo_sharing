{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "lib": [
      "es2023"
    ],
    "module": "CommonJS",
    "target": "es2022",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "node10",
    "outDir": "dist/"
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}